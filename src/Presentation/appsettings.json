{
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://0.0.0.0:8083",
        "Protocols": "Http1AndHttp2"
      }
    }
  },
  "Grpc": {
    "AccountServiceAddress": "http://0.0.0.0:8082"
  },
  "Postgres": {
    "Host": "localhost",
    "Port": 5403,
    "Database": "postgres",
    "Username": "postgres",
    "Password": "postgres",
    "SslMode": "Prefer"
  },
  "MessagePersistence": {
    "SchemaName": "message_persistence",
    "Publisher": {
      "Default": {
        "BatchSize": 10,
        "PollingDelay": "00:00:00.500"
      }
    }
  },
  "Kafka": {
    "Host": "localhost:8001",
    "Consumers": {
      "PassengerCreatedMessage": {
        "IsDisabled": false,
        "Topic": "passenger.added",
        "Group": "Passenger-Service",
        "Inbox": {
          "BatchSize": 10,
          "PollingDelay": "00:00:00.500"
        }
      }
    }
  }
}